<h1>Login</h1>

<%= link_to "Sign in with Facebook", "/auth/facebook", id: "sign_in", rel: :external %>

<%= link_to "Sign Up!", new_user_path %>
<%= form_tag '/login' do %>

  Email: <%= text_field_tag :email %>
  Password: <%= password_field_tag :password %>
  <%= submit_tag "Submit" %>

<% end %>